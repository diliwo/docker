        "SUPPORTS_BOOLEAN_DATA_TYPE": "N",
        "PRESERVE_RESERVED_WORD_CASE": "Y",
        "FORCE_IDENTIFIERS_TO_UPPERCASE": "N",
        "PORT_NUMBER": "${PgPortNumber}"
      },
      "username": "${PgUsername}"
    }
  },
  "name": "Post"
}