{
  "rdbms": {
    "POSTGRESQL": {
      "databaseName": "${PgDatabaseName}",
      "pluginId": "POSTGRESQL",
      "accessType": 0,
      "hostname": "${PgHostname}",
      "password": "${PgPassword}",
      "pluginName": "PostgreSQL",
      "port": "${PgPortNumber}",
      "attributes": {
        "USE_POOLING": "N",
        "IS_CLUSTERED": "N",
