  <xsd:complexType name="ContractualRelationshipAdditionalCriteria">
    <xsd:annotation>
      <xsd:appinfo>v1.5: complexType added</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="workSectorCode" type="common:WorkSectorCode" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: criterion added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="worksGlobalProjectPurposeCode" type="crr:WorksGlobalProjectPurposeCode" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: criterion added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="declarationStatus" type="ContractualRelationshipDeclarationStatus" minOccurs="0"/>
      <xsd:element name="isConstructionWorkWithAsbestos" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Indicates if the construction work involves asbestos works
            0/false = No
            1/true = Yes</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="isObligedToRegister" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Indicates if the workers of a site are obliged to register their presence on the site. Depends on the total cost of the works
            0/false = No
            1/true = Yes</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intervalSearchType" type="ContractualRelationshipIntervalSearchType" minOccurs="0"/>
      <xsd:element name="intervalSearchStartDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Interval search start date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intervalSearchEndDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Interval search end date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="ContractualRelationshipDeclarationStatus">
    <xsd:annotation>
      <xsd:appinfo>v1.5: simpleType added</xsd:appinfo>
