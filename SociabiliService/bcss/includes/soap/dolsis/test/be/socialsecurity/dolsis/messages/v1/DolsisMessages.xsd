            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fetchSize" minOccurs="0" default="10">
            <xsd:simpleType>
              <xsd:restriction base="xsd:int">
                <xsd:minInclusive value="1"/>
                <xsd:maxInclusive value="2000"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="sortCriterion" type="PresenceRegistrationSortCriterion" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The field and direction by which the results are sorted, default is by id asc</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="PresenceRegistrationSortField">
    <xsd:annotation>
      <xsd:appinfo>v1.6: simpleType added</xsd:appinfo>
      <xsd:documentation>Sort field for a presence registration</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="presence_registration_id"/>
      <xsd:enumeration value="registration_date"/>
      <xsd:enumeration value="creation_date"/>
      <xsd:enumeration value="last_validation_status"/>
      <xsd:enumeration value="contractual_relationship_declaration_id"/>
      <xsd:enumeration value="cbe_number"/>
      <xsd:enumeration value="channel"/>
      <xsd:enumeration value="sender_user_id"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PresenceRegistrationSortCriterion">
    <xsd:annotation>
      <xsd:appinfo>v1.6: complexType added</xsd:appinfo>
      <xsd:documentation>Sort criterion for a presence registration</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="PresenceRegistrationSortField">
        <xsd:attribute default="asc" name="direction" type="SortDirection"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="SortDirection">
    <xsd:annotation>
      <xsd:appinfo>v1.6: simpleType added</xsd:appinfo>
      <xsd:documentation>Sort direction</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="asc"/>
      <xsd:enumeration value="desc"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PresenceRegistrations">
    <xsd:annotation>
      <xsd:appinfo>v1.6: complexType added</xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="common:PagingList">
        <xsd:sequence>
          <xsd:element name="presenceRegistration" type="pr:PresenceRegistration" maxOccurs="2000">
            <xsd:annotation>
              <xsd:documentation>List of presence registrations</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- abstract types -->
  <xsd:complexType abstract="true" name="AbstractRequestType">
    <xsd:annotation>
      <xsd:appinfo>v0.11: added securityContext, v0.14: added informationSupplier/CBSS from deprecated AbstractResponseType</xsd:appinfo>
      <xsd:documentation>Base complex type for all request messages.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="informationCustomer" type="cbss:InformationCustomerType"/>
      <xsd:choice minOccurs="0">
        <xsd:element name="informationSupplier" type="cbss:InformationSupplierType"/>
        <xsd:element name="informationCBSS" type="cbss:InformationCBSSType"/>
      </xsd:choice>
      <xsd:element name="securityContext" type="SecurityContextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SecurityContextType">
    <xsd:annotation>
      <xsd:appinfo>v0.11: new complex type</xsd:appinfo>
      <xsd:documentation/>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="service" type="ServiceType">
        <xsd:annotation>
          <xsd:documentation>A further qualification of the service consumer performing the data consultation (eg department of the institution with CBE number from informationConsumer).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="legalContext" type="cbss:LegalContextType">
        <xsd:annotation>
          <xsd:documentation>The purpose of the data consultation</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="role" type="SecurityRole">
        <xsd:annotation>
          <xsd:documentation>The role assigned to the service consumer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="forceDataAccess" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Consumers who are required to integrate into the CBSS registries prior to data consultation, must explicitly communicate this by setting this value.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="ServiceType">
    <xsd:annotation>
      <xsd:appinfo>v0.11: new complex type</xsd:appinfo>
      <xsd:documentation/>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="SecurityRole">
    <xsd:annotation>
      <xsd:appinfo>v0.11: new complex type</xsd:appinfo>
      <xsd:documentation/>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Agent"/>
      <xsd:enumeration value="Inspector"/>
      <xsd:enumeration value="Monitoring"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType abstract="true" name="AbstractSearchCriteriaType">
    <xsd:annotation>
      <xsd:appinfo>v0.9: new abstract base type for all criteria objects</xsd:appinfo>
      <xsd:documentation>Base complex type for all criteria messages.</xsd:documentation>
    </xsd:annotation>
  </xsd:complexType>
</xsd:schema>