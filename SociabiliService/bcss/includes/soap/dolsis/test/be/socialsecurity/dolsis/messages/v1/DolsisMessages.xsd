<xsd:schema version="1.7" targetNamespace="http://socialsecurity.be/dolsis/messages/v1" xmlns="http://socialsecurity.be/dolsis/messages/v1" attributeFormDefault="unqualified" elementFormDefault="unqualified" xmlns:cbss="http://kszbcss.fgov.be/types/common/v3" xmlns:common="http://socialsecurity.be/dolsis/types/v1" xmlns:crr="http://socialsecurity.be/dolsis/contractualrelationshipregistry/types/v1" xmlns:pr="http://socialsecurity.be/dolsis/presenceregistration/types/v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import namespace="http://kszbcss.fgov.be/types/common/v3" schemaLocation="../../../../fgov/kszbcss/types/common/v3/CommonV3.xsd"/>
  <xsd:import namespace="http://socialsecurity.be/dolsis/types/v1" schemaLocation="../../types/v1/DolsisTypes.xsd"/>
  <xsd:import namespace="http://socialsecurity.be/dolsis/contractualrelationshipregistry/types/v1" schemaLocation="../../contractualrelationshipregistry/types/v1/ContractualRelationshipRegistryTypes.xsd"/>
  <xsd:import namespace="http://socialsecurity.be/dolsis/presenceregistration/types/v1" schemaLocation="../../presenceregistration/types/v1/PresenceRegistrationTypes.xsd"/>
  <!-- Case 3 - findEmployersByWorker -->
  <xsd:complexType name="FindEmployersByWorkerRequestType">
    <xsd:annotation>
      <xsd:documentation>Find the employers a person has worked for.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="EmployersByWorkerCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FindEmployersByWorkerResponseType">
    <xsd:annotation>
      <xsd:documentation>Returns the list of employers the person has worked for.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FindEmployersByWorkerRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="EmployersByWorker" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmployersByWorkerCriteria">
    <xsd:annotation>
      <xsd:documentation>Search based on worker social security number and optional period filter.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="ssin" type="cbss:SsinType"/>
          <xsd:element name="scope" type="common:CentralPersonnelFileSearchScope" default="active">
            <xsd:annotation>
              <xsd:appinfo>v0.6: renamed all scope to active</xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="period" type="cbss:PeriodType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmployersByWorker">
    <xsd:sequence>
      <xsd:element name="person" type="common:Person"/>
      <xsd:element name="employment" type="EmployersByWorkerRecord" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EmployersByWorkerRecord">
    <xsd:complexContent>
      <xsd:extension base="common:EmploymentRecord">
        <xsd:sequence>
          <xsd:element name="employer" type="common:Employer"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- Case 4 - getEmploymentDetail -->
  <xsd:complexType name="GetEmploymentDetailRequestType">
    <xsd:annotation>
      <xsd:documentation>Retrieve all details for the employment of a worker at a single employer</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="EmploymentDetailCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="GetEmploymentDetailResponseType">
    <xsd:annotation>
      <xsd:documentation>Full details of the employment</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="GetEmploymentDetailRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="EmploymentDetail" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmploymentDetailCriteria">
    <xsd:annotation>
      <xsd:appinfo>v1.2: complexType changed</xsd:appinfo>
      <xsd:documentation>Search based on identification of worker, employer and quarter</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="nossRegistrationNumber" type="common:NOSSRegistrationNbr"/>
          <xsd:element name="ssin" type="cbss:SsinType"/>
          <xsd:element name="quarter" type="common:Quarter"/>
          <xsd:element name="nbrOfQuarters" type="NumberOfQuarters" minOccurs="0" default="1"/>
          <xsd:element name="employmentRecords" type="xsd:boolean" minOccurs="0" default="true">
            <xsd:annotation>
              <xsd:appinfo>v1.4: added boolean employmentRecords</xsd:appinfo>
              <xsd:documentation>Boolean indicating whether RIP should be consulted</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dmfaRecords" type="xsd:boolean" minOccurs="0" default="true">
            <xsd:annotation>
              <xsd:appinfo>v1.4: added boolean dmfaRecords</xsd:appinfo>
              <xsd:documentation>Boolean indicating whether DMFA should be consulted</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="NumberOfQuarters">
    <xsd:annotation>
      <xsd:appinfo>v1.2: simpleType added</xsd:appinfo>
      <xsd:documentation>The number of quarters to return information for (up to 4)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="4"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="EmploymentDetail">
    <xsd:sequence>
      <xsd:element name="person" type="common:Person" minOccurs="0"/>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="employmentRecord" type="common:EmploymentRecord" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="employmentRecordStatus" type="cbss:StatusType">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added to inform incomplete information</xsd:appinfo>
          <xsd:documentation>Normal status is DATA_FOUND/MSG00000/OK or NO_DATA_FOUND/MSG00000/OK. Status is NO_DATA_FOUND/MSG00000/OK if user has no access to employment data. Status is NO_RESULT/DOLS0023/TOO_MANY_RIP_RESULTS when there are more than
            2000 results.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dmfaRecord" type="common:DmfaNaturalPersonDeclarationRecord" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:appinfo>v0.6: changed DmfaRecord to DmfaNaturalPersonDeclarationRecord</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dmfaRecordStatus" type="cbss:StatusType">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added to inform incomplete information</xsd:appinfo>
          <xsd:documentation>Normal status is DATA_FOUND/MSG00000/OK or NO_DATA_FOUND/MSG00000/OK. Status is NO_DATA_FOUND/DOLS0024/INVALID_REGNR when nossRegistrationNbr is invalid for DMFA consultation. Status is NO_DATA_FOUND/MSG00000/OK if user has no
            access to dmfa data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Case 5 - GetEmployerDetail -->
  <xsd:complexType name="GetEmployerDetailRequestType">
    <xsd:annotation>
      <xsd:documentation>Get all details about an employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="EmployerDetailCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="GetEmployerDetailResponseType">
    <xsd:annotation>
      <xsd:documentation>The employer data from the NOSS employer registry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="GetEmployerDetailRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="EmployerDetail" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmployerDetailCriteria">
    <xsd:annotation>
      <xsd:documentation>Search based on identification number of employer</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:choice>
          <xsd:element name="nossRegistrationNbr" type="common:NOSSRegistrationNbr"/>
          <xsd:element name="cbeNumber" type="cbss:CbeNumberType">
            <xsd:annotation>
              <xsd:appinfo>v0.6: renamed companyId to cbeNumber</xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ssin" type="cbss:SsinType"/>
          <xsd:element name="vatNumber" type="common:VatNumber"/>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmployerDetail">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="additionalData" type="common:EmployerAdditionalData"/>
      <xsd:element name="socialSecretariat" type="common:EmployerSocialSecretariat" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: should be optional element</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="representative" type="common:EmployerRepresentative" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Case 6 - Find Employers -->
  <xsd:complexType name="FindEmployersRequestType">
    <xsd:annotation>
      <xsd:documentation>Find employers by phonetic or regional search criteria</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="EmployerCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FindEmployersResponseType">
    <xsd:annotation>
      <xsd:documentation>Returns a summary list of employers.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FindEmployersRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="Employers" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EmployerCriteria">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed from EmployerSearchCriteria for consistence</xsd:appinfo>
      <xsd:documentation>Search based on phonetic or regional criteria.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="scope" type="EmployerSearchScope"/>
          <xsd:choice>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="address" type="xsd:string"/>
          </xsd:choice>
          <xsd:element name="zipCode" type="xsd:string" minOccurs="0"/>
          <xsd:element name="regionScope" type="RegionScope"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="EmployerSearchScope">
    <xsd:annotation>
      <xsd:documentation>The partition of the NOSS employer registry to search</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="noss">
        <xsd:annotation>
          <xsd:appinfo>v1.6: Deprecated. Replaced by noss-active</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="noss-legacy">
        <xsd:annotation>
          <xsd:appinfo>v1.6: Deprecated. Replaced by noss-deleted</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="noss-pla">
        <xsd:annotation>
          <xsd:appinfo>v1.6: Deprecated. Replaced by ossss</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="noss-active">
        <xsd:annotation>
          <xsd:appinfo>v1.6: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="noss-deleted">
        <xsd:annotation>
          <xsd:appinfo>v1.6: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ossss">
        <xsd:annotation>
          <xsd:appinfo>v1.6: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RegionScope">
    <xsd:annotation>
      <xsd:documentation>The regions to search for.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Belgium"/>
      <xsd:enumeration value="BelgianZipCode"/>
      <xsd:enumeration value="Foreign"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Employers">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Case 7 - findWorkersByEmployer -->
  <xsd:complexType name="FindWorkersByEmployerRequestType">
    <xsd:annotation>
      <xsd:documentation>Find workers for a single employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="WorkersByEmployerCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FindWorkersByEmployerResponseType">
    <xsd:annotation>
      <xsd:documentation>Returns the employment records of the employed workers (if search result not too big)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FindWorkersByEmployerRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="WorkersByEmployer" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WorkersByEmployerCriteria">
    <xsd:annotation>
      <xsd:documentation>Search based on employer identification, optional period and optional search filters.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="nossRegistrationNbr" type="common:NOSSRegistrationNbr"/>
          <xsd:element name="scope" type="common:CentralPersonnelFileSearchScope"/>
          <xsd:element name="period" type="cbss:PeriodType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WorkersByEmployer">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="employment" type="WorkersByEmployerRecord" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="WorkersByEmployerRecord">
    <xsd:complexContent>
      <xsd:extension base="common:EmploymentRecord">
        <xsd:sequence>
          <xsd:element name="person" type="common:PersonSummary"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- Case 8 - findDmfaRecordsByEmployer -->
  <xsd:complexType name="FindDmfaRecordsByEmployerRequestType">
    <xsd:annotation>
      <xsd:documentation>Find the DMFA records of an employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="DmfaRecordCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FindDmfaRecordsByEmployerResponseType">
    <xsd:annotation>
      <xsd:documentation>The DMFA records (person related) of a single employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FindDmfaRecordsByEmployerRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="DmfaRecordsByEmployer" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaRecordCriteria">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed from DmfaRecordsCriteria</xsd:appinfo>
      <xsd:documentation>Search based on employer identification number, quarter and optional filters</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="nossRegistrationNbr" type="common:NOSSRegistrationNbr"/>
          <xsd:element name="quarter" type="common:Quarter"/>
          <xsd:element name="curatorship" type="common:Curatorship"/>
          <xsd:element name="filter" type="DmfaRecordFilter" minOccurs="0"/>
          <xsd:element name="offset" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>v0.14: added offset to page large DMFA results to avoid performance problems within DMFA queries</xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaRecordsByEmployer">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="record" type="common:DmfaNaturalPersonDeclarationRecord" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:appinfo>v0.6: changed QualifiedDmfaRecord to DmfaNaturalPersonDeclarationRecord</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nextOffset" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: added to avoid performance problems with DMFA queries</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DmfaRecordFilter">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed from DmfaRecordsFilter</xsd:appinfo>
      <xsd:documentation>Filters to reduce the number of searched DMFA records</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="workerType" type="DmfaWorkerType"/>
        <xsd:element name="contractType" type="common:ContractType"/>
        <xsd:element name="workerStatus" type="common:WorkerStatus" default="seasonal_worker">
          <xsd:annotation>
            <xsd:appinfo>v0.6: type changed from boolean to enum for foward changes</xsd:appinfo>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="numberUnemployedDays" type="xsd:int"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="DmfaWorkerType">
    <xsd:annotation>
      <xsd:documentation>Nature of a worker</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="labourer"/>
      <xsd:enumeration value="employee"/>
      <xsd:enumeration value="student"/>
      <xsd:enumeration value="earlyRetirement"/>
      <xsd:enumeration value="dismissedStatutoryWorker"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- Case 9: getDmfaStatistics -->
  <xsd:complexType name="GetDmfaStatisticsByEmployerRequestType">
    <xsd:annotation>
      <xsd:appinfo>v0.7: new operation</xsd:appinfo>
      <xsd:documentation>Get the DMFA statistics overview of an employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="DmfaRecordCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="GetDmfaStatisticsByEmployerResponseType">
    <xsd:annotation>
      <xsd:appinfo>v0.7: new operation</xsd:appinfo>
      <xsd:documentation>The DMFA statistics overview of a single employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="GetDmfaStatisticsByEmployerRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="DmfaStatisticsByEmployer" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaStatisticsByEmployer">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="curatorship" type="common:Curatorship">
        <xsd:annotation>
          <xsd:documentation>
            <xsd:appinfo>v0.18: added curatorship field</xsd:appinfo>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalWorkerRecords" type="xsd:double"/>
      <xsd:element name="totalPartTime" type="xsd:double"/>
      <xsd:element name="totalWorkMan" type="xsd:double"/>
      <xsd:element name="totalEmployees" type="xsd:double"/>
      <xsd:element name="totalStudents" type="xsd:double"/>
      <xsd:element name="totalServiceNbrOfDays" type="xsd:double"/>
      <xsd:element name="totalEarlyRetirement" type="xsd:double"/>
      <xsd:element name="totalDismissedStatutoryWorkers" type="xsd:double"/>
      <xsd:element name="totalAuthorizedAbsencesNbrOfDays" type="xsd:double"/>
      <xsd:element name="totalEconomicalUnemploymentNbrOfDays" type="xsd:double"/>
      <xsd:element name="totalBadWeatherTemporaryUnemploymentNbrOfDays" type="xsd:double"/>
      <xsd:element name="totalOtherTemporaryUnemploymentNbrOfDays" type="xsd:double"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Case 10 - findDmfaContributionsUnrelatedToNaturalPerson -->
  <xsd:complexType name="FindDmfaContributionsUnrelatedToNaturalPersonRequestType">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed operation</xsd:appinfo>
      <xsd:documentation>Find the DMFA records (not related to persons) of an employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractRequestType">
        <xsd:sequence>
          <xsd:element name="criteria" type="DmfaContributionUnrelatedToNPCriteria"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FindDmfaContributionsUnrelatedToNaturalPersonResponseType">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed operation</xsd:appinfo>
      <xsd:documentation>The DMFA records (not related to persons) of a single employer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FindDmfaContributionsUnrelatedToNaturalPersonRequestType">
        <xsd:sequence>
          <xsd:element name="status" type="cbss:StatusType"/>
          <xsd:element name="result" type="DmfaContributionsUnrelatedToNP" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaContributionUnrelatedToNPCriteria">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed from WorkerUnrelatedDmfaRecordsCriteria</xsd:appinfo>
      <xsd:documentation>Search DMFA contribution not related to natureal person</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="AbstractSearchCriteriaType">
        <xsd:sequence>
          <xsd:element name="nossRegistrationNbr" type="common:NOSSRegistrationNbr"/>
          <xsd:element name="startQuarter" type="common:Quarter"/>
          <xsd:element name="endQuarter" type="common:Quarter"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaContributionsUnrelatedToNP">
    <xsd:sequence>
      <xsd:element name="employer" type="common:Employer"/>
      <xsd:element name="record" type="common:DmfaUnrelatedToNaturalPersonDeclarationRecord" minOccurs="0" maxOccurs="unbounded">
