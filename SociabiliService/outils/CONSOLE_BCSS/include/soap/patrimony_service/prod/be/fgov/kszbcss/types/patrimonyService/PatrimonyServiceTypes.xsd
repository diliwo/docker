	<xs:complexType name="ParcelIdentificationType">
		<xs:sequence>
			<xs:element name="bisNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
bisnumber
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="cadastralDivision" type="tns:CodeDescriptionType">
				<xs:annotation>
					<xs:documentation>
description the cadastral division
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exponentLetter" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
exponentletter
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="exponentNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
exponentnumber
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fractionDenominator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
Denominator for the common parts
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="partNumber">
				<xs:annotation>
					<xs:documentation>
partnumber (P0000 -&gt; P9999)
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
						<xs:pattern value="(P[0-9][0-9][0-9][0-9])"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="primaryNumber">
				<xs:annotation>
					<xs:documentation>
primary number (grondnummer)
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="section">
				<xs:annotation>
					<xs:documentation>
section
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="precad" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
precad
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
						<xs:pattern value="([s,r,S,R])"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
