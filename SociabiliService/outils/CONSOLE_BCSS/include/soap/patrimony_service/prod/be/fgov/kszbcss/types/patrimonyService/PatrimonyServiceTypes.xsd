		<xs:sequence>
			<xs:element name="party" type="tns:PartyType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RightType">
		<xs:sequence>
			<xs:element name="type" type="tns:CodeDescriptionType">
				<xs:annotation>
					<xs:documentation>
description for the type of the businessright
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fractNum">
				<xs:annotation>
					<xs:documentation>
Enominator of the shares of a normalised business right
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fractDenom">
				<xs:annotation>
					<xs:documentation>
Denominator of the shares of a normalised business right
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="part" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
part not specified
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="partSurface" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
No demarcation
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="surface" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
surface
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:totalDigits value="19"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="expiredDate" type="tns:YearOrDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
expiredate of the businessright
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OwnersType">
		<xs:choice>
			<xs:element name="individual" type="com:SsinType"/>
			<xs:element name="community" type="tns:CommunityType"/>
			<xs:element name="business" type="com:EnterpriseNumberType"/>
			<xs:element name="association" type="tns:AssociationType"/>
			<xs:element name="anonymous">
				<xs:annotation>
					<xs:documentation>
String of the anonymous owners and their businessrights
                        (# en 2 rechthebbenden -BE 1/4- #)
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="IndividualType"/>
	<xs:complexType name="CommunityType">
		<xs:sequence>
			<xs:element name="ssin" type="com:SsinType" minOccurs="2" maxOccurs="2"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssociationType">
		<xs:sequence>
			<xs:element name="ssin" type="com:SsinType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IdentifierType">
		<xs:choice/>
	</xs:complexType>
	<!--===================== Parcel state ====================-->
	<xs:complexType name="SituationIdType">
		<xs:sequence>
			<xs:element name="id" type="xs:long" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SituationIdentifierType">
		<xs:sequence>
			<xs:element name="current" type="xs:long">
				<xs:annotation>
					<xs:documentation>
SitPatrimoniale.oIdfTech =&gt; input voor
                        getPropertyByPropertyId
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="previous" type="tns:SituationIdType" minOccurs="0"/>
			<xs:element name="next" type="tns:SituationIdType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CalculatedMarketValueType">
		<xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
Date when the market value was calculated
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="min" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
Calculated minimal market value for sale
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="max" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
calculated maximal market value for sale
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="average" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
The calculated market value of the propertystate.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FiscalDateType">
		<xs:sequence>
			<xs:element name="begin" type="tns:YearOrDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
fiscal startdate of the propertystate
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end" type="tns:YearOrDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
fiscal enddate of the propertystate
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LegalDateType">
		<xs:sequence>
			<xs:element name="begin" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
juridical begindate of the propertystate
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
juridacal enddate of the propertystate
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VersionDateType">
		<xs:sequence>
			<xs:element name="begin" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
timestamp for the validation of the creation of the
                        dossier
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
timestamp for the validation of the mutation of the
                        dossier
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancel" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
timestamp for the validation of the canceling of the
                        dossier
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--===================== Complex types ====================-->
	<xs:complexType name="CodeDescriptionType">
		<xs:sequence>
			<xs:element name="code">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="description" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="abbreviation" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element name="zipCode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="cityCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="cityName" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="street" type="tns:CodeDescriptionType"/>
			<xs:element name="number" type="xs:integer" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!--===================== Simple types ====================-->
	<xs:complexType name="IdentityType">
		<xs:choice>
			<xs:element name="person" type="tns:PersonType"/>
			<xs:element name="enterprise" type="tns:EnterpriseType"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="RightsType">
		<xs:sequence>
			<xs:element name="right" type="tns:RightType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DivisionsType">
		<xs:sequence>
			<xs:element name="division" type="tns:DivisionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DivisionType">
		<xs:sequence>
			<xs:element name="number" type="xs:string"/>
			<xs:element name="location" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchCriteriaForDivisionType">
		<xs:sequence>
			<xs:element name="enterpriseNumber" type="com:EnterpriseNumberType"/>
			<xs:element name="history" type="xs:boolean" minOccurs="0"/>
			<xs:element name="language" type="com:LanguageType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchCriteriaForEnterpriseType">
		<xs:sequence>
			<xs:element name="enterpriseNumber" type="com:EnterpriseNumberType"/>
			<xs:element name="division" type="xs:integer"/>
			<xs:element name="withCoOwners" type="xs:boolean" minOccurs="0"/>
			<xs:choice minOccurs="0">
				<xs:element name="fullHistory" type="xs:boolean"/>
				<xs:element name="period" type="tns:PeriodType"/>
			</xs:choice>
			<xs:element name="language" type="com:LanguageType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="YearOrDateType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-2][0-9]{3}-(0[1-9]|1[0-2])-(0[1-9]|[1-2][0-9]|3[0-1])"/>
			<xs:pattern value="[1-2][0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DecretesType">
		<xs:sequence>
			<xs:element name="decrete" type="tns:CodeDescriptionType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
description for the cadastral income decrete
                        </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MotifType">
		<xs:sequence>
			<xs:element name="description"/>
			<xs:element name="notification" minOccurs="0" maxOccurs="3">
				<xs:annotation>
					<xs:documentation>
description of the motivNotif1
               </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:CodeDescriptionType">
							<xs:attribute name="order">
								<xs:simpleType>
									<xs:restriction base="xs:short">
										<xs:minInclusive value="1"/>
										<xs:maxInclusive value="3"/>
										<xs:totalDigits value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FloorDetailType">
		<xs:sequence>
			<xs:element name="floorTo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
specifications of property in a building without a base
                        act/deed
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="floorNumberUndergrond" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
amount of floors underground
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="floorNumberAboveground" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
amount of floors above ground
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="floorFrom" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
specifications of property in a building without a base
                        act/deed
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="floor" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
floor
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="floorSituation" type="tns:CodeDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
description of the floor situation
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InformationsType">
		<xs:sequence>
			<xs:element name="information" type="tns:ParcelInformationType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
