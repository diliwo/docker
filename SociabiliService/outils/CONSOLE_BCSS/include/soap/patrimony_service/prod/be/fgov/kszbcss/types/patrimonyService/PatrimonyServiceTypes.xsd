	<xs:complexType name="ParcelInformationType">
		<xs:sequence>
			<xs:element name="cadastralIncome" type="tns:CadastralIncomeType" minOccurs="0"/>
			<xs:element name="location" type="tns:LocationType" minOccurs="0"/>
