    <xsd:annotation>
      <xsd:appinfo>v0.6: removed type 'both' because it meant 'unspecified'</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="noss"/>
      <xsd:enumeration value="noss_pla">
