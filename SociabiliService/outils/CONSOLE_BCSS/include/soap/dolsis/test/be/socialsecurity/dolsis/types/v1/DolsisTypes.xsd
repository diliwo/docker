  <xsd:complexType name="NOSSRegistrationNbr">
    <xsd:annotation>
      <xsd:appinfo>v0.6: base type changed to string because numbers in construction are alphanumeric, removed restrictions and merged with NossTemporaryRegistrationNbr.</xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" type="NOSSRegistrationNbrType" use="optional">
          <xsd:annotation>
            <xsd:appinfo>v0.6: optional because type 'both' meant in fact 'unspecified'</xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="subType" type="NOSSRegistrationNbrSubType" use="optional">
          <xsd:annotation>
            <xsd:appinfo>v0.7: optional attribute to qualify registration number better, permits more exact lookup</xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
