  <xsd:complexType name="PostingPlaceOfWorkRelation">
    <xsd:annotation>
      <xsd:documentation>Relation between a place of work and a declaration</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="placeOfWork" type="PostingPlaceOfWork"/>
      <xsd:element name="period" type="cbss:OpenPeriodType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="PostingPlaceOfWorkType">
    <xsd:annotation>
      <xsd:documentation>The decision type</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="unknown">
        <xsd:annotation>
          <xsd:appinfo>limosa code: 0</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="area">
        <xsd:annotation>
          <xsd:appinfo>limosa code: 1</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="company">
        <xsd:annotation>
          <xsd:appinfo>limosa code: 2</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="construction_site">
        <xsd:annotation>
          <xsd:appinfo>limosa code: 3</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="boat">
        <xsd:annotation>
          <xsd:appinfo>limosa code: 4</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PostingDocumentStatusType">
    <xsd:annotation>
      <xsd:documentation>The status type</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="unknown">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 0</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="valid">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 1</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="appeal">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 2</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="on_hold">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 3</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="submitted">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 4</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="decision">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 5</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="classified_noresult">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 6</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="cancelled">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 7</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="deleted">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 8</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="refused">
        <xsd:annotation>
          <xsd:appinfo>limosa code = 9</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType abstract="true" name="PagingList">
    <xsd:annotation>
      <xsd:appinfo>v1.6: sequence removed</xsd:appinfo>
    </xsd:annotation>
    <xsd:attribute name="maxResults" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>The total number of results</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="nextOffset" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>The offset of the next page (if there is a next page)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="PostingRelationPagingList">
    <xsd:complexContent>
      <xsd:extension base="PagingList">
        <xsd:sequence>
          <xsd:element name="relation" type="PostingRelation" minOccurs="0" maxOccurs="2000"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PostingRelation">
    <xsd:annotation>
      <xsd:appinfo>mapping: be.smalsmvm.limosa.ejb.actors.ActorDataRelation</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="period" type="cbss:OpenPeriodType"/>
      <xsd:element name="type" type="PostingRelationType"/>
      <xsd:choice>
        <xsd:element name="person" type="Person"/>
        <xsd:element name="employer" type="Employer"/>
      </xsd:choice>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:long"/>
  </xsd:complexType>
  <xsd:simpleType name="PostingRelationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="created_from"/>
      <xsd:enumeration value="occupation"/>
      <xsd:enumeration value="mandataire"/>
      <xsd:enumeration value="employe"/>
      <xsd:enumeration value="independant"/>
      <xsd:enumeration value="stagiaire"/>
      <xsd:enumeration value="stagiaire_independant"/>
      <xsd:enumeration value="occupation_mandataire_ou_dirigeant"/>
      <xsd:enumeration value="employer_belgian_client">
        <xsd:annotation>
          <xsd:appinfo>v1.6: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="LimosaSearchScope">
    <xsd:annotation>
      <xsd:documentation>The partition of the Limosa registry to search</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="official"/>
      <xsd:enumeration value="all"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PostingDocumentRelationPagingList">
    <xsd:complexContent>
      <xsd:extension base="PagingList">
        <xsd:sequence>
          <xsd:element name="relation" type="PostingDocumentRelation" minOccurs="0" maxOccurs="2000"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PostingDocumentRelation">
    <xsd:annotation>
      <xsd:appinfo>v0.7: type split into PostingDocumentRelation for POW-Document relation PostingDocumentParticipation for Actor-Document relation</xsd:appinfo>
      <xsd:appinfo>mapping: be.smalsmvm.limosa.ejb.files.POWRelation</xsd:appinfo>
      <xsd:documentation>Relation between an actor and a declaration</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="period" type="cbss:OpenPeriodType"/>
      <xsd:element name="summary" type="PostingDocumentSummary" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PostingDocumentParticipationPagingList">
    <xsd:complexContent>
      <xsd:extension base="PagingList">
        <xsd:sequence>
          <xsd:element name="participation" type="PostingDocumentParticipation" minOccurs="0" maxOccurs="2000"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PostingDocumentParticipation">
    <xsd:annotation>
      <xsd:appinfo>v1.2: complexType changed</xsd:appinfo>
      <xsd:appinfo>v0.7: type split into PostingDocumentRelation for POW-Document relation PostingDocumentParticipation for Actor-Document relation</xsd:appinfo>
      <xsd:appinfo>mapping: be.smalsmvm.limosa.ejb.files.Participation</xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PostingDocumentRelation">
        <xsd:sequence>
          <xsd:element name="type" type="ParticipationType"/>
          <xsd:choice minOccurs="0">
            <xsd:element name="person" type="Person"/>
            <xsd:element name="employer" type="Employer"/>
            <xsd:element name="partnerInstitution" type="PartnerInstitution">
              <xsd:annotation>
                <xsd:appinfo>v1.2: added</xsd:appinfo>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="ParticipationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="worker"/>
      <xsd:enumeration value="trainee"/>
      <xsd:enumeration value="self_employed"/>
      <xsd:enumeration value="self_employed_trainee"/>
      <xsd:enumeration value="belgian_institution"/>
      <xsd:enumeration value="belgian_user"/>
      <xsd:enumeration value="foreign_institution"/>
      <xsd:enumeration value="foreign_employer"/>
      <xsd:enumeration value="representative"/>
      <xsd:enumeration value="salary_paid_by"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BelgianClientParticipationType">
    <xsd:restriction base="ParticipationType">
      <xsd:enumeration value="belgian_user"/>
      <xsd:enumeration value="belgian_institution"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PostedWorkerParticipationType">
    <xsd:restriction base="ParticipationType">
      <xsd:enumeration value="worker"/>
      <xsd:enumeration value="trainee"/>
      <xsd:enumeration value="self_employed"/>
      <xsd:enumeration value="self_employed_trainee"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ForeignEmployerParticipationType">
    <xsd:restriction base="ParticipationType">
      <xsd:enumeration value="foreign_employer"/>
      <xsd:enumeration value="foreign_institution"/>
      <xsd:enumeration value="self_employed"/>
      <xsd:enumeration value="self_employed_trainee"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Activity">
    <xsd:sequence>
      <xsd:element name="code" type="xsd:int"/>
      <xsd:element name="sector" type="xsd:int"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ContactData">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" type="ContactDataType" use="optional"/>
        <xsd:attribute name="startDate" type="xsd:date" use="optional">
          <xsd:annotation>
            <xsd:appinfo>v0.7: added for limosa</xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="ContactDataType">
    <xsd:annotation>
      <xsd:documentation>Type of the contact data (mail, private phone, ...)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="phone"/>
      <xsd:enumeration value="fax"/>
      <xsd:enumeration value="cell_phone"/>
      <xsd:enumeration value="voip"/>
      <xsd:enumeration value="email"/>
      <xsd:enumeration value="other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Alias">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" type="AliasType" use="optional"/>
        <xsd:attribute name="startDate" type="xsd:date" use="optional">
          <xsd:annotation>
            <xsd:appinfo>v0.8: added start date for alias</xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AliasType">
    <xsd:annotation>
      <xsd:documentation>The alias type</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="alias_name"/>
      <xsd:enumeration value="abbreviation"/>
      <xsd:enumeration value="sign"/>
      <xsd:enumeration value="other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CountryCode">
    <xsd:annotation>
      <xsd:documentation>Numerical country codes defined by statbel.fgov.be (Country nomenclature GEOBEL)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="100"/>
      <xsd:maxInclusive value="999">
        <xsd:annotation>
          <xsd:appinfo>v0.14: 999 added for unknown</xsd:appinfo>
        </xsd:annotation>
      </xsd:maxInclusive>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="FeeNumber">
    <xsd:restriction base="xsd:long"/>
  </xsd:simpleType>
  <xsd:simpleType name="CentralPersonnelFileSearchScope">
    <xsd:annotation>
      <xsd:appinfo>renamed from CentralPersonnelFileScope</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="active">
        <xsd:annotation>
          <xsd:documentation>Search the active periods of the worker in the central file registry</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="activeInPeriod">
        <xsd:annotation>
          <xsd:documentation>Search the active periods of a worker during a period.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="mutations">
        <xsd:annotation>
          <xsd:documentation>Search all mutations (entries/exists) of a worker within a period , inclusive cancelled ones</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Quarter">
    <xsd:restriction base="xsd:int">
      <xsd:totalDigits value="5"/>
      <xsd:pattern value="\d{4}(1|2|3|4)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Name">
    <xsd:annotation>
      <xsd:appinfo>v0.20: new complex type</xsd:appinfo>
      <xsd:documentation>a translated string with optional language information</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="language" type="cbss:LanguageType" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="OpenPeriod">
    <xsd:annotation>
      <xsd:appinfo>v1.6: complexType added</xsd:appinfo>
      <xsd:documentation>Period containining an optional begin date and an optional end date.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="beginDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="endDate" type="xsd:date" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="WorkSectorCode">
    <xsd:annotation>
      <xsd:appinfo>v1.5: simpleType added</xsd:appinfo>
      <xsd:appinfo>v1.6: moved simpleType to DolsisTypes.xsd so that it can be shared by ContractualRelationshipRegistry and PresenceRegistration</xsd:appinfo>
      <xsd:documentation>Work sector
        1 = Construction work
        2 = Security / Guarding
        3 = Meat</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int"/>
  </xsd:simpleType>
</xsd:schema>