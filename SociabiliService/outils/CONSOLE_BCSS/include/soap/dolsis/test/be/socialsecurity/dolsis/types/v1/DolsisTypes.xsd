  <xsd:simpleType name="Tolerance">
    <xsd:annotation>
      <xsd:appinfo>v0.6: changed maxInclusive to 99</xsd:appinfo>
      <xsd:documentation>The tolerance must be 0 or a number between 1 and 12 if only a birth year and month are specified or between 1 and 99 if only a birth year is specified.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99"/>
    </xsd:restriction>
