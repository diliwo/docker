      <xsd:enumeration value="building_owner"/>
      <xsd:enumeration value="building_atce"/>
      <xsd:enumeration value="contractor"/>
      <xsd:enumeration value="subcontractor"/>
      <xsd:enumeration value="social_secretariat"/>
      <xsd:enumeration value="curator"/>
      <xsd:enumeration value="pla">
        <xsd:annotation>
          <xsd:appinfo>v1.6: Deprecated. Replaced by ossss</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ossss">
        <xsd:annotation>
          <xsd:appinfo>v1.6: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="temporary"/>
    </xsd:restriction>
