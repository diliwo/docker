      <xsd:element name="welfareBenefitTheoreticalAmount" type="xsd:long" minOccurs="0"/>
      <xsd:element name="nbrMonthsDecimals" type="xsd:int" minOccurs="0"/>
      <xsd:element name="incompleteMonthNbrDays" type="xsd:int" minOccurs="0"/>
      <xsd:element name="incompleteMonthReason" type="xsd:int" minOccurs="0"/>
      <xsd:element name="floorApplicationNotion" type="xsd:int" minOccurs="0"/>
    </xsd:sequence>
