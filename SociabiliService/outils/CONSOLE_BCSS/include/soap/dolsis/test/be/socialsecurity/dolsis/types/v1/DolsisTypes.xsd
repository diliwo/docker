    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DmfaComplementaryIndemnityContribution">
    <xsd:annotation>
      <xsd:appinfo>v1.2: complexType added</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="contributionWorkerCode" type="xsd:int"/>
      <xsd:element name="contributionType" type="xsd:int"/>
      <xsd:element name="contributionAmount" type="xsd:long"/>
      <xsd:element name="complementaryIndemnityAmountAdjustNotion" type="xsd:int"/>
