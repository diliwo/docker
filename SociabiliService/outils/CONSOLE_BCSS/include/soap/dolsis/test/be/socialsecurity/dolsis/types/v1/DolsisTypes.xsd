<xsd:schema version="1.7" targetNamespace="http://socialsecurity.be/dolsis/types/v1" xmlns="http://socialsecurity.be/dolsis/types/v1" attributeFormDefault="unqualified" elementFormDefault="unqualified" xmlns:cbss="http://kszbcss.fgov.be/types/common/v3" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import namespace="http://kszbcss.fgov.be/types/common/v3" schemaLocation="../../../../fgov/kszbcss/types/common/v3/CommonV3.xsd"/>
  <xsd:complexType name="Address">
    <xsd:annotation>
      <xsd:appinfo>v0.15: removed boolean mainAddress, mainAddress is the Person::address now</xsd:appinfo>
      <xsd:appinfo>v0.20: street and city type changed to Name + maxOccurs 3</xsd:appinfo>
      <xsd:documentation>Represents an address</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="street" type="Name" minOccurs="0" maxOccurs="3"/>
      <xsd:element name="houseNbr" type="xsd:string" minOccurs="0"/>
      <xsd:element name="postBox" type="xsd:string" minOccurs="0"/>
      <xsd:element name="zipCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="city" type="Name" minOccurs="0" maxOccurs="3"/>
      <xsd:element name="country" type="xsd:string" minOccurs="0"/>
      <xsd:element name="countryCode" type="CountryCode" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: added for completeness</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="startDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="type" type="AddressType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="AddressType">
    <xsd:annotation>
      <xsd:documentation>Represents the type of address</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="private"/>
      <xsd:enumeration value="professional"/>
      <xsd:enumeration value="branch_office"/>
      <xsd:enumeration value="registered_office"/>
      <xsd:enumeration value="residence">
        <xsd:annotation>
          <xsd:appinfo>v1.2: enumeration added</xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="FamilyMember">
    <xsd:sequence>
      <xsd:element name="person" type="PersonSummary">
        <xsd:annotation>
          <xsd:appinfo>v0.6: reused summary instead of ssin/lastName/firstName</xsd:appinfo>
          <xsd:appinfo>v0.17: removed period</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="positionCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Household status / Position dans le ménage / Stand in het gezin
            1 = householder / chef de ménage / gezinshoofd
            2 = spouse / conjoint / echtgenoot, echtgenote
            3 = son, daughter / fils, fille / zoon, dochter
            4 = son-in-law, daughter-in-law / gendre, bru / schoonzoon, schoondochter
            5 = grandson, granddaughter / petit-fils, petite-fille / kleinzoon, kleindochter
            6 = father, mother / père, mère / vader, moeder
            7 = father-in-law, mother-in-law / beau-père, belle-mère / schoonvader, schoonmoeder
            8 = grandfather, grandmother / grand-père, grand-mère / grootvader, grootmoeder
            9 = brother, sister / frère, soeur / broer, zuster
            10 = brother-in-law, sister-in-law / beau-frère, belle-soeur / schoonbroer, schoonzuster
            11 = relative / apparenté(e) / verwant, verwante
            12 = not related / non-apparenté(e) / zonder familieverband
            13 = stepson, stepdaughter / beau-fils, belle-fille / stiefzoon, stiefdochter
            14 = great-grandson, great-granddaughter / arrière petit-fils, arrière petite-fille / achterkleinzoon, achterkleindochter
            15 = aunt, uncle / tante, oncle / tante, oom
            16 = niece, nephew (relatives in the 3rd degree) / nièce, neveu (3ème degré de parenté) / nicht, neef (verwantschap : 3de graad)
            17 = cousin (relatives in the 4th degree) / nièce, neveu (4ème degré de parenté) / nicht, neef (verwantschap : 4de graad)
            20 = communities, homes / communautés, homes / gemeenschappen, tehuizen</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="beginDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="endDate" type="xsd:date" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute default="false" name="householder" type="xsd:boolean" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="Person">
    <xsd:annotation>
      <xsd:appinfo>v0.6: added: id and changed fields to optional for Limosa use cases</xsd:appinfo>
      <xsd:appinfo>v0.16: added source attribute</xsd:appinfo>
      <xsd:appinfo>v0.19: added second name</xsd:appinfo>
      <xsd:appinfo>v0.20: birthplace type changed to Name + maxOccurs 3</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ssin" type="cbss:SsinType" minOccurs="0"/>
      <xsd:element name="lastName" type="xsd:string"/>
      <xsd:element name="firstName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="secondName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="birthDate" type="cbss:IncompleteDateType" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type changed for Limosa use cases</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="deceaseDate" type="cbss:IncompleteDateType" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.7: added field</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="birthPlace" type="Name" minOccurs="0" maxOccurs="3"/>
      <xsd:element name="gender" type="GenderType" minOccurs="0"/>
      <xsd:element name="maritalStatus" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>0 = Onbekend, 00 = Onbekend, 10 = Ongehuwd, 20 = Gehuwd, 25 = Nietigverklaring van huwelijk, 26 = Putatief huwelijk, 30 = Weduwnaar/weduwe, 40 = Echtgescheiden, 41 = Echtgescheiden, 50 = Gescheiden van T &amp; B, 51 =
            Gescheiden van T &amp; B, 60 = Verstoting, 80 = Partnerschap, 81 = Beëindigd partnerschap, 90 = Onbepaald</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="maritalStatusInceptionDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="maritalStatusPartner" type="PersonSummary" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nationality" type="CountryCode" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: reused existing simple type</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="address" type="Address" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added for limosa use cases</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:long" use="optional"/>
    <xsd:attribute name="source" type="PersonDataSource" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="PersonSummary">
    <xsd:annotation>
      <xsd:appinfo>v0.6: renamed from PersonReference</xsd:appinfo>
      <xsd:appinfo>v0.7: removed status because CBSS will not complete information</xsd:appinfo>
      <xsd:appinfo>v0.15: set ssin optional for Limosa compatibility</xsd:appinfo>
      <xsd:appinfo>v0.16: added source attribute</xsd:appinfo>
      <xsd:appinfo>v0.19: added second name</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ssin" type="cbss:SsinType" minOccurs="0"/>
      <xsd:element name="lastName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="firstName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="secondName" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="source" type="PersonDataSource" use="optional"/>
  </xsd:complexType>
  <xsd:simpleType name="PersonDataSource">
    <xsd:annotation>
      <xsd:documentation>v0.16: added simple type</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="nr">
        <xsd:annotation>
          <xsd:documentation>national register</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="bis">
        <xsd:annotation>
          <xsd:documentation>BIS register (CBSS)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="rad">
        <xsd:annotation>
          <xsd:documentation>Registre des radiés/ Register der geschrapten (CBSS)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="Employer">
    <xsd:annotation>
      <xsd:appinfo>v0.6: added: id and changed fields to optional for Limosa use cases</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string"/>
      <xsd:element name="nossRegistrationNumber" type="NOSSRegistrationNbr" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: optional because not always known</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nossTemporaryRegistrationNumber" type="NOSSRegistrationNbr" minOccurs="0"/>
      <xsd:element name="cbeNumber" type="cbss:CbeNumberType" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: renamed cbeNumber like type</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ssin" type="cbss:SsinType" minOccurs="0"/>
      <xsd:element name="feeNumber" type="FeeNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added for Limosa</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="vatNumber" type="VatNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.7: moved from EmployerAdditionalData for Limosa use cases</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="address" type="Address" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: A Limosa case was found without an address</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="countryCode" type="CountryCode" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: added the country code for a Limosa employer can be different than the main address</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="underCuratorshipBeginDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nossDeletionDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element added</xsd:appinfo>
          <xsd:appinfo>v1.6: Moved element nossDeletionDate from type EmployerAdditionalData to type Employer</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:long" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="EmployerAdditionalData">
    <xsd:annotation>
      <xsd:appinfo>v0.6: moved to common dolsis types</xsd:appinfo>
      <xsd:documentation>Employer data from the NOSS employer registry</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="identificationNumber7M" type="xsd:long" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type more specific + fix typo</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="buildingRegistrationNumber" type="xsd:long" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type more specific + optional</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="administrativeCode" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: should be optional element</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="administrativeSystem" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type more specific</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="employerClass" type="xsd:int" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type more specific</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="importanceCode" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: type more specific, should be optional element</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="activityCode" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.18: added data field</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="legalForm" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: should be optional element</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nossMembershipDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: should be optional element</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nossDeletionDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: Deprecated. Moved to type Employer</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="phoneNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="faxNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="email" type="xsd:string" minOccurs="0"/>
      <xsd:element name="bankruptcyDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: moved from EmployerRepresentative</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EmployerSocialSecretariat">
    <xsd:annotation>
      <xsd:appinfo>v0.6: moved to common dolsis types</xsd:appinfo>
      <xsd:documentation>Details on the employers social secretariat</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ssaNbr" type="SocialSecretariatNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: optional because the info does not always exist in Reportorium</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: optional because the info does not always exist in Reportorium</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="address" type="Address" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: optional because the info does not always exist in Reportorium</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="membershipSince" type="Quarter" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element changed (minOccurs added)</xsd:appinfo>
          <xsd:appinfo>v1.6: optional because the info does not always exist in Reportorium</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EmployerRepresentative">
    <xsd:annotation>
      <xsd:appinfo>v0.6: moved to common dolsis types</xsd:appinfo>
      <xsd:documentation>Details on the employers representative (mandatary/curator)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string"/>
      <xsd:element name="address" type="Address"/>
      <xsd:element name="beginDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.14: renamed because not all representatives are curator</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EmploymentRecord">
    <xsd:sequence>
      <xsd:element name="lastDimonaAction" type="DimonaAction">
        <xsd:annotation>
          <xsd:appinfo>v0.6: create enumeration type for dimona actions</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="inServiceDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: date and hours must be expressed separately to support absent time information, start is optional</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="inServiceHour" type="xsd:time" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v 0.6: new element hour</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="outServiceDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: date and hours must be expressed separately to support absent time information, start is optional</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="outServiceHour" type="xsd:time" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: new element hour</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="workerType" type="EmploymentWorkerType">
        <xsd:annotation>
          <xsd:appinfo>v0.6: create enumeration type for worjker types</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="subEntityNbr" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: renamed</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="jointCommissionNbr" type="JointCommissionNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="studentPlaceOfWork" type="StudentPlaceOfWork" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: subtype created</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usingEmployer" type="UsingEmployer" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: subtype created</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nbrOfPlannedWorkingDays" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cancelled" type="xsd:boolean">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dimonaPeriodId" type="xsd:long" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v0.6: added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="creationDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element added</xsd:appinfo>
          <xsd:documentation>Optional for backward compatibility</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lastUpdateDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.6: element added</xsd:appinfo>
          <xsd:documentation>Optional for backward compatibility</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="DimonaAction">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="in"/>
      <xsd:enumeration value="out"/>
      <xsd:enumeration value="modification"/>
      <xsd:enumeration value="cancellation"/>
      <xsd:enumeration value="deletion"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="EmploymentWorkerType">
    <xsd:annotation>
      <xsd:appinfo>v1.7: simpleType changed</xsd:appinfo>
      <xsd:documentation>Worker type in central personnel file (rip)
      Possible values: stu, stx, ext, ivt, rta, oth, bcw, dwd, tea, tri, flx</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="3"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="StudentPlaceOfWork">
    <xsd:annotation>
      <xsd:appinfo>v0.6: new complex type</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="employerName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="address" type="Address" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UsingEmployer">
    <xsd:annotation>
      <xsd:appinfo>v0.6: new complex type</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string" minOccurs="0"/>
      <xsd:element name="cbeNumber" type="cbss:CbeNumberType" minOccurs="0"/>
      <xsd:element name="nossRegistrationNbr" type="NOSSRegistrationNbr" minOccurs="0"/>
      <xsd:element name="jointCommissionNbr" type="JointCommissionNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:appinfo>v1.2: element added</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType abstract="true" name="DmfaDeclarationRecord">
    <xsd:annotation>
      <xsd:appinfo>v0.6: changed to mimic DMFA data structure</xsd:appinfo>
      <xsd:appinfo>v0.19: changed formReceptionDate to xsd:dateTime</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="quarter" type="Quarter"/>
      <xsd:element name="curatorship" type="Curatorship"/>
      <xsd:element name="formIdentification" type="xsd:string" minOccurs="0"/>
      <xsd:element name="formReceptionDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="system5" type="xsd:boolean"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DmfaNaturalPersonDeclarationRecord">
    <xsd:annotation>
      <xsd:appinfo>v1.2: complexType changed</xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DmfaDeclarationRecord">
        <xsd:sequence>
          <xsd:element name="person" type="PersonSummary" minOccurs="0"/>
          <xsd:element name="workerRecord" type="DmfaWorkerRecord" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="ticketNbrLastSituation" type="TicketNumber">
            <xsd:annotation>
              <xsd:appinfo>v1.2: added</xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaUnrelatedToNaturalPersonDeclarationRecord">
    <xsd:complexContent>
      <xsd:extension base="DmfaDeclarationRecord">
        <xsd:sequence>
          <xsd:element name="contribution" type="DmfaContributionUnrelatedToNP" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DmfaWorkerRecord">
    <xsd:annotation>
      <xsd:appinfo>v0.6: new complex type</xsd:appinfo>
