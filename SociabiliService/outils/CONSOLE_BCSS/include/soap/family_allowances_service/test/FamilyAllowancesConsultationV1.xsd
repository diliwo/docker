<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSpy v2008 rel. 2 sp2 (http://www.altova.com) by bcss (KSZ-BCSS) --><!-- ============================================================================================================         * File Name :             FamilyAllowancesConsultation.xsd         * Author :                  Catherine Cocu         * Owner :                   BCSS/KSZ         * Creation date :           03-03-11         * Last revision date :     24-08-11         * Status :                     Planned         * Description :              Service for retrieving Family Allowances.											Service hosted by BCSS         * Current version :         0.3         * Revision History : 				0.1 						initial version ;				0.2						Added multiplicity for beneficiaryChild and recipient	            0.3						- Updated name of allocated  by recipient and attributedand by entitled Person. 											- Added a legalContext for the request											- Renamed inss by ssin============================================================================================================= --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://kszbcss.fgov.be/types/FamilyAllowancesConsult" xmlns:com="http://kszbcss.fgov.be/types/common/v3" targetNamespace="http://kszbcss.fgov.be/types/FamilyAllowancesConsult" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="001.02">	<xsd:import namespace="http://kszbcss.fgov.be/types/common/v3" schemaLocation="be/fgov/kszbcss/types/common/CommonV3.xsd"/>	<xsd:complexType name="FamilyAllowancesConsultRequestType">		<xsd:sequence>			<xsd:element name="informationCustomer" type="InformationCustomerType"/>			<xsd:element name="informationCBSS" type="InformationCBSSType" minOccurs="0"/>			<xsd:element name="legalContext" type="LegalContextType"/>			<xsd:element name="ssin" type="SSINType">				<xsd:annotation>					<xsd:documentation xml:lang="FR">NISS Numero d'identification de la securite social</xsd:documentation>					<xsd:documentation xml:lang="NL">INSZ - identificatienummer bij sociale zekerheid</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="searchPeriod" type="SearchPeriodType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="FamilyAllowancesConsultResponseType">		<xsd:sequence>			<xsd:element name="informationCustomer" type="InformationCustomerType"/>			<xsd:element name="informationCBSS" type="InformationCBSSType"/>			<xsd:element name="ssin" type="SSINType">				<xsd:annotation>					<xsd:documentation xml:lang="FR">NISS Numero d'identification de la securite social</xsd:documentation>					<xsd:documentation xml:lang="NL">INSZ - identificatienummer bij sociale zekerheid</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="searchPeriod" type="SearchPeriodType"/>			<xsd:element name="status" type="com:StatusType">				<xsd:annotation>					<xsd:documentation>Indicates the overall status of the treatment.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="cadafDossier" type="CadafDossierType" minOccurs="0" maxOccurs="999"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="CadafDossierType">		<xsd:sequence>			<xsd:element name="cadafID" type="CadafIDType"/>			<xsd:element name="actorList">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="entitledPerson" type="EntitledPersonType"/>						<xsd:element name="recipientList" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Recipient list</xsd:documentation>							</xsd:annotation>							<xsd:complexType>								<xsd:sequence>									<xsd:element name="recipient" type="RecipientType" maxOccurs="unbounded"/>								</xsd:sequence>							</xsd:complexType>						</xsd:element>						<xsd:element name="beneficiaryChildList" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Recipient list</xsd:documentation>							</xsd:annotation>							<xsd:complexType>								<xsd:sequence>									<xsd:element name="beneficiaryChild" type="BeneficiaryChildType" maxOccurs="unbounded"/>								</xsd:sequence>							</xsd:complexType>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="CadafIDType">		<xsd:annotation>			<xsd:documentation>Stamnummer</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="cbfFundID">				<xsd:annotation>					<xsd:documentation>Fondsnummer</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:pattern value="[0-9][0-9][0-9]"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="officeNbr">				<xsd:annotation>					<xsd:documentation>Bureaunummer</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:length value="2"/>						<xsd:whiteSpace value="preserve"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="dossierID">				<xsd:annotation>					<xsd:documentation>Dossiernummer</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:minLength value="1"/>						<xsd:maxLength value="18"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="RecipientType">		<xsd:annotation>			<xsd:documentation>Recipient</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ssin" type="SSINType"/>			<xsd:element name="type">				<xsd:annotation>					<xsd:documentation>Allocated Type (1 or 2).</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:integer">						<xsd:enumeration value="1"/>						<xsd:enumeration value="2"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="paymentPeriod" type="PaymentType" minOccurs="0" maxOccurs="999"/>			<xsd:element name="birthBonus" minOccurs="0" maxOccurs="20">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="paymentDate" type="xsd:date"/>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="BeneficiaryChildType">		<xsd:annotation>			<xsd:documentation>Beneficiary Child</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ssin" type="SSINType"/>			<xsd:element name="paymentPeriod" type="PaymentType" minOccurs="0" maxOccurs="999"/>			<xsd:element name="adoptionBonus" minOccurs="0" maxOccurs="20">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="paymentDate" type="xsd:date"/>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="EntitledPersonType">		<xsd:annotation>			<xsd:documentation>EntitledPerson</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ssin" type="SSINType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="returnCode">		<xsd:annotation>			<xsd:documentation xml:lang="FR">Code retour</xsd:documentation>			<xsd:documentation xml:lang="NL">Returncode</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="6"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:complexType name="SearchPeriodType">		<xsd:annotation>			<xsd:documentation>Format : The year and month (YYYY-MM)</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="beginDate" type="xsd:gYearMonth"/>			<xsd:element name="endDate" type="xsd:gYearMonth" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="status" type="com:StatusType"/>	<xsd:complexType name="InformationType">		<xsd:sequence>			<xsd:element name="code">				<xsd:annotation>					<xsd:documentation>Extra information identifier.</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="64"/>						<xsd:enumeration value="ALL_OK"/>						<xsd:enumeration value="NO_RESULT"/>						<xsd:enumeration value="NO_DATA_FOUND"/>						<xsd:enumeration value="DATA_FOUND"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="description">				<xsd:annotation>					<xsd:documentation>Extra information value.</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="256"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="SSINType">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\p{N}{11}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="PaymentType">		<xsd:sequence>			<xsd:element name="beginDate" type="xsd:date"/>			<xsd:element name="endDate" type="xsd:date" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="InformationCBSSType">		<xsd:annotation>			<xsd:documentation>Information block from CBSS.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ticketCBSS" type="UUIDType"/>			<xsd:element name="timestampReceive" type="xsd:dateTime">				<xsd:annotation>					<xsd:documentation>Time request was received by CBSS.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="timestampReply" type="xsd:dateTime">				<xsd:annotation>					<xsd:documentation>Time reply was sent by CBSS.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="InformationCustomerType">		<xsd:annotation>			<xsd:documentation>Information block that can be used by the customer for diagnostic use.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ticket" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Client can assign their own ticket reference for the request. Not mandatory.</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="32"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="timestampSent" type="xsd:dateTime" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Timestamp when client sent request.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="customerIdentification" type="OrganizationIdentificationType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="OrganizationIdentificationType">		<xsd:choice>			<xsd:element name="cbeNumber" type="CbeNumberType"/>			<xsd:sequence>				<xsd:element name="sector">					<xsd:simpleType>						<xsd:restriction base="xsd:unsignedShort">							<xsd:maxInclusive value="999"/>						</xsd:restriction>					</xsd:simpleType>				</xsd:element>				<xsd:element name="institution">					<xsd:simpleType>						<xsd:restriction base="xsd:unsignedShort">							<xsd:maxInclusive value="999"/>						</xsd:restriction>					</xsd:simpleType>				</xsd:element>			</xsd:sequence>		</xsd:choice>	</xsd:complexType>	<xsd:complexType name="FamilyAllowancesConsultFaultType">		<xsd:sequence>			<xsd:element name="informationCustomer" type="InformationCustomerType"/>			<xsd:element name="informationCBSS" type="InformationCBSSType"/>			<xsd:element name="error" type="MessageType" maxOccurs="30"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="MessageType">		<xsd:sequence>			<xsd:element name="severity" type="SeverityType"/>			<xsd:element name="reasonCode">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="16"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="diagnostic">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="256"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="authorCode">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:maxLength value="64"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="information" type="InformationType" minOccurs="0" maxOccurs="5"/>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="SeverityType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="INFORMATION"/>			<xsd:enumeration value="WARNING"/>			<xsd:enumeration value="FATAL"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="CbeNumberType">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\d{10}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="UUIDType">		<xsd:restriction base="xsd:string">			<xsd:pattern value="[\da-fA-F]{8}-[\da-fA-F]{4}-[\da-fA-F]{4}-[\da-fA-F]{4}-[\da-fA-F]{12}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="LegalContextType">		<xsd:restriction base="xsd:string">			<xsd:maxLength value="64"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>